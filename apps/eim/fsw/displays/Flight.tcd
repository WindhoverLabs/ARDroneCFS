<html>
  <head>
    <title>Flight</title>
  </head>
  <body>
    <div class="table-wrapper">
      <table class="table table-striped table-condensed">
        <caption>Flight Display</caption>
        <thead>
          <tr>
            <th>Name</th>
            <th>Value</th>
          </tr>
        </thead>
        <tbody>
          <tr>
            <td>Ground Time</td>
            <td><span data-sage='{"tlm":{"Name":"EIMARD2_CmdCounter.GndTime"}}'>---</span></td>
          </tr>
          <tr>
            <td>UAS Time</td>
            <td><span data-sage='{"tlm":{"Name":"EIMARD2_CmdCounter.UASTime"}}'>---</span></td>
          </tr>
          <tr>
            <td>Command Counter</td>
            <td><span data-sage='{"tlm":{"Name":"EIMARD2_CmdCounter"}}'>---</span></td>
          </tr>
          <tr>
            <td>Error Counter</td>
            <td><span data-sage='{"tlm":{"Name":"EIMARD2_ErrCounter"}}'>---</span></td>
          </tr>
          <tr>
            <td span="2">
              <button class="btn btn-primary btn-xs" type="button" data-sage='{"cmd":{"Name":"EIM_SetFlatTrim"}}'>Set Flat Trim</button><br></br>
              <button class="btn btn-primary btn-xs" type="button" data-sage='{"cmd":{"Name":"EIM_Launch"}}'>Launch</button><br></br>
              <button class="btn btn-primary btn-xs" type="button" data-sage='{"cmd":{"Name":"EIM_EmergencyStop"}}'>EMERGENCY STOP</button><br></br>
              <button class="btn btn-primary btn-xs" type="button" data-sage='{"cmd":{"Name":"EIM_EmergencyStopReset"}}'>Emergency Reset</button><br></br>
              <button class="btn btn-primary btn-xs" type="button" data-sage='{"cmd":{"Name":"EIM_Vertical"}}'>Vertical</button><br></br>
              <button class="btn btn-primary btn-xs" type="button" data-sage='{"cmd":{"Name":"EIM_Land"}}'>Land</button><br></br>
              <button class="btn btn-primary btn-xs" type="button" data-sage='{"cmd":{"Name":"EIM_Hover"}}'>Hover</button><br></br>
              <button class="btn btn-primary btn-xs" type="button" data-sage='{"cmd":{"Name":"EIM_Roll"}}'>Roll</button><br></br>
              <button class="btn btn-primary btn-xs" type="button" data-sage='{"cmd":{"Name":"EIM_Pitch"}}'>Pitch</button><br></br>
              <button class="btn btn-primary btn-xs" type="button" data-sage='{"cmd":{"Name":"EIM_Yaw"}}'>Yaw</button><br></br>
              <button class="btn btn-primary btn-xs" type="button" data-sage='{"cmd":{"Name":"EIM_Vertical"}}'>Vertical</button>
            </td>
          </tr>
        </tbody>
      </table>
    </div>
  </body>
</html>
